<?xml version="1.0" encoding="UTF-8"?>
<database>
    <element audit="true" label="Survey" max_length="40" name="x_alabs_pex_survey" type="collection">
        <element default="true" label="Active" max_length="40" name="active" type="boolean"/>
        <element label="description" max_length="4000" name="description" type="translated_text"/>
        <element label="End note" max_length="10000" name="end_note" type="translated_html"/>
        <element label="introduction" max_length="10000" name="introduction" type="translated_html"/>
        <element attributes="no_sort=true,slushbucket_ref_no_expand=true" label="Locations" max_length="1024" name="locations" reference="x_alabs_pex_location_survey" reference_qual="javascript:new SurveyUtils().FilterLocation()" type="glide_list" use_reference_qualifier="advanced"/>
        <element label="Name" mandatory="true" max_length="200" name="name" type="translated_text"/>
        <element attributes="ignore_filter_on_new=true" default="javascript:global.getNextObjNumberPadded();" label="Number" max_length="40" name="number" read_only="true" type="string"/>
        <element label="Parent" max_length="32" name="parent" reference="x_alabs_pex_survey" type="reference"/>
        <element choice="3" default="draft" label="State" max_length="40" name="state" type="choice">
            <choice>
                <element label="Draft" sequence="0" value="draft"/>
                <element label="Waiting Approved" sequence="1" value="waiting_approved"/>
                <element label="Published" sequence="2" value="published"/>
                <element label="Completed" sequence="3" value="completed"/>
                <element label="Canceled" sequence="4" value="canceled"/>
            </choice>
        </element>
        <element label="Type" mandatory="true" max_length="32" name="type" reference="x_alabs_pex_survey_type" reference_qual="active=true^EQ" reference_qual_condition="active=true^EQ" type="reference"/>
        <element label="Valid Until" mandatory="true" max_length="40" name="valid_until" type="glide_date"/>
        <index name="index">
            <element name="parent"/>
        </index>
        <index name="index2">
            <element name="type"/>
        </index>
    </element>
</database>
