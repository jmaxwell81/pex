<?xml version="1.0" encoding="UTF-8"?>
<database>
    <element audit="true" label="Action" max_length="40" name="x_alabs_pex_action_plan_action" type="collection">
        <element label="Action Plan" max_length="32" name="action_plan" reference="x_alabs_pex_action_plan" type="reference"/>
        <element label="Notes" max_length="4000" name="notes" type="journal_input"/>
        <element attributes="ignore_filter_on_new=true" default="javascript:global.getNextObjNumberPadded();" label="Number" max_length="40" name="number" read_only="true" type="string"/>
        <element label="Question" max_length="32" name="question_instance" reference="x_alabs_pex_instance_survey_question" type="reference"/>
        <element choice="3" default="pending" label="State" max_length="40" name="state" type="choice">
            <choice>
                <element label="Pending" sequence="0" value="pending"/>
                <element label="Completed" sequence="1" value="completed"/>
                <element label="Canceled" sequence="2" value="canceled"/>
            </choice>
        </element>
        <index name="index">
            <element name="action_plan"/>
        </index>
        <index name="index2">
            <element name="question_instance"/>
        </index>
    </element>
</database>
